<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="@dimen/spacing.extra_small"
        android:top="@dimen/spacing.x_extra_small">
        <shape>
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/spacing.small" />
        </shape>
    </item>
    <item
        android:bottom="@dimen/spacing.extra_small"
        android:right="@dimen/spacing.x_extra_small">
        <shape>
            <solid android:color="@color/primary_color" />
            <corners android:radius="@dimen/spacing.small" />
            <stroke
                android:width="@dimen/spacing.x_extra_small"
                android:color="@color/login_custom_button_enable_boarder_color" />
        </shape>
    </item>
</layer-list>
