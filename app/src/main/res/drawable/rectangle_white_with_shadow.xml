<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:left="2dp"
        android:top="2dp">
        <shape>
            <solid android:color="@color/primary_midnight" />
            <corners android:radius="@dimen/spacing.small" />
        </shape>
    </item>
    <item
        android:bottom="2dp"
        android:right="2dp">
        <shape>
            <solid android:color="@color/white" />
            <corners android:radius="@dimen/spacing.small" />
            <stroke
                android:width="2dp"
                android:color="@color/primary_midnight" />
        </shape>
    </item>
</layer-list>